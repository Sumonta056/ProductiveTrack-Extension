{
    "name": "Productivity Tracker",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Tracks users' browsing habits and provides insights",
    "permissions": ["tabs", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png"
    },
    "icons": {
      "128": "logo.png"
    }
  }